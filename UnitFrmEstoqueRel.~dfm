  object Query1: TZQuery
    Connection = dm.ZConnection1
    SQL.Strings = (
      
        'select E.produto,P.nome as nomeproduto,E.und,sum(E.qtd) as qtd,s' +
        'um(E.vlrtotal) as Total,F.nome'
      'from movestoque E, produtos P, fornecedores F'
      
        'where (E.data='#39'2012/12/26'#39') and (E.produto=P.codigo) and (E.favo' +
        'recido=F.codigo)'
      'group by P.nome'
      'order by F.nome,P.nome')
    Params = <>
    object Query1produto: TIntegerField
      FieldName = 'produto'
      Required = True
    end
    object Query1nomeproduto: TStringField
      FieldName = 'nomeproduto'
      Required = True
      Size = 50
    end
    object Query1und: TStringField
      FieldName = 'und'
      Required = True
      Size = 5
    end
    object Query1qtd: TFloatField
      FieldName = 'qtd'
    end
    object Query1Total: TFloatField
      FieldName = 'Total'
    end
    object Query1nome: TStringField
      FieldName = 'nome'
      Size = 70
    end
  end
